10 GRAPHICS 0
20 DIM FIL$(1),FIL2$((INT(ADR(FIL$)/2048)+1)*2048-ADR(FIL$)-1)
30 DIM BUFFER$(384),MISSILES$(128),PLAYER0$(128),PLAYER1$(128),PLAYER2$(128),PLAYER3$(128)
35 H=40:DIM FR1$(H),FR2$(H),FR3$(H),FR4$(H):DIM BLANK$(8):BLANK$="\00\00\00\00\00\00\00\00"
40 CHBAS=57344:DIM FT$(9):FT$="\02\05 \0F\05 \02\10 ":DIM DM(7)
50 CHSET=(PEEK(106)-8)*256
55 EXEC INITPM
60 EXEC COPYCHRS
70 EXEC READCHARS
80 EXEC INIT
90 CLS:POKE 752,1:DY=1:F=1:FI=1
130 POS. 2,13:FOR I=0 TO 37:PRINT CHR$(8);:NEXT I
140 XP=36
150 DO
160   REM POKE 764, 255 
170   KEY = PEEK(764)
180   IF KEY=33 THEN J=1
190   IF J=1
206     YP=DM(FI)*8
210     FI=FI+1
220     IF YP<=0:YP=0: POKE 764,255:J=0:ENDIF
230     IF FI>7 THEN FI=1
240   ENDIF
250   EXEC DINO
260   XP=XP-1
270   IF XP<=0: XP=36:NCACTUS=RAND(3):ENDIF
280   EXEC CACTUS
290   PAUSE 5
300 LOOP
319 END
320 PROC READCHARS
325 FCHAR=65+7:NCHRS=7
330   OFFSET=FCHAR*8
340   FOR I=OFFSET TO NCHRS*8+OFFSET
350     READ A:POKE CHSET+I,A
360   NEXT I
370   POKE 756,CHSET/256
380 ENDPROC
400 PROC COPYCHRS
410     MOVE CHBAS,CHSET,1023
420 ENDPROC
500 PROC DINO
502     DOFF=36
505     F=F+3:IF F>8:F=1:ENDIF
520     PLAYER0$(DOFF-YP)=FR1$
525     PLAYER1$(DOFF-YP)=FR2$
530     PLAYER2$(DOFF-YP)=FR3$
550 ENDPROC
800 PROC CACTUS
810   IF XP=1
820     POS. 1,11:PRINT "   ";:POS. 1,12:PRINT "   ";
830   ELSE
840     POS. XP,11:PRINT " ";CHR$(9+NCACTUS*2);" ";
850     POS. XP,12:PRINT " ";CHR$(10+NCACTUS*2);" ";
860   ENDIF
870   PAUSE 5
880 ENDPROC
900 PROC INIT
910     DM(1)=1:DM(2)=2:DM(3)=3:DM(4)=3:DM(5)=2:DM(6)=1:DM(7)=0
920 ENDPROC
930 PROC INITPM
940 FR1$(1)=BLANK$:FR1$(33)=BLANK$
945 FR2$(1)=BLANK$:FR2$(33)=BLANK$
946 FR3$(1)=BLANK$:FR3$(33)=BLANK$
950     FOR I=1 TO 2*8:READ A:FR1$(I+16,I+16)=CHR$(A):NEXT I
960     FOR I=1 TO 3*8:READ A:FR2$(I+8,I+8)=CHR$(A):NEXT I
970     FOR I=1 TO 2*8:READ A:FR3$(I+8,I+8)=CHR$(A):NEXT I
980     POKE 54279,ADR(BUFFER$)/256:POKE 559,46:POKE 53277,3
985     POKE 704,15:POKE 705,15:POKE 706,15
990     POKE 53260,2
1000     POKE 53248,50:POKE 53249,50+8
1005     POKE 53250,50+16
1010 ENDPROC
