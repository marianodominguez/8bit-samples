10 GRAPHICS 8+16:COLOR 1:SETCOLOR 2,0,0:SETCOLOR 1,1,14:PI=3.141592
15 XP=-1:YP=-1
20 P1=COS(PI/6):P2=COS(PI/3)
30 FOR X=-90 TO 90 STEP 5
40   FOR Y=-80 TO 80 STEP 5
50     Z=20*(COS(X/20)*SIN(Y/20))
55     XX=XP:YY=YP
60     XP=-P1*X+P1*Y+160
70     YP=-P2*X-P2*Y+Z+91
80     PLOT XP,YP
85     IF XX>=0:DRAWTO XX,YY:ENDIF 
90   NEXT Y
95   XP=-1:YP=-1
100 NEXT X
