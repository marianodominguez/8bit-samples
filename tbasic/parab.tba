10 GRAPHICS 8+16:SETCOLOR 1,1,14:COLOR 1:SETCOLOR 2,0,0
20 PLOT 160,0:DRAWTO 160,191
30 PLOT 0,96:DRAWTO 319,96
40 FOR X=-159 TO 159
50   Y=2*X*X-20*X-4000
60   XS=(X+159)
70   YS=Y*(90/(160*160))
80   YS=96-YS
100   IF YS>0 AND YS<191 THEN PLOT XS,YS
110 NEXT X
120 TEXT 150,1,"Y"
130 TEXT 310,97,"X"
220 PAUSE 500
